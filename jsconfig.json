{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES6",
    "module": "commonJS",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "paths": {
      "@pages/*": [
        "./src/pages/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@redux/*": [
        "./src/redux/*"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@shared/*": [
        "./src/shared/*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}